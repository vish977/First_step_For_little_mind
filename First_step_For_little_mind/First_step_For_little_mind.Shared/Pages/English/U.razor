					</div>

					<div class="card card-hover shadow s7 mb-4" onmouseover="cardAudioPlay('audioAstronout')"
						 onmouseout="cardpauseAudio('audioAstronout')">
						<img src="_content/First_step_For_little_mind.Shared/Image/English/astronout.jpeg" class="card-img-top" alt="English Image">
						<div class="card-body text-center">
							<h5 class="card-title">Aestronout</h5>
						</div>
						<audio id="audioAstronout">
							<source src="_content/First_step_For_little_mind.Shared/voice/astronout.mp3" type="audio/mpeg">
						</audio>
					</div>

					<div class="card card-hover shadow s8 mb-4" onmouseover="cardAudioPlay('audioArrow')"
						 onmouseout="cardpauseAudio('audioArrow')">
						<img src="_content/First_step_For_little_mind.Shared/Image/English/arrow.png" class="card-img-top" alt="English Image">
						<div class="card-body text-center">
							<h5 class="card-title">Arrow</h5>
						</div>
						<audio id="audioArrow">
							<source src="_content/First_step_For_little_mind.Shared/voice/arrow.mp3" type="audio/mpeg">
						</audio>
					</div>
				</div>
			</div>

			<button class="carousel-control-next" type="button" data-bs-target="#carouselExample" data-bs-slide="next">
				<span><img src="_content/First_step_For_little_mind.Shared/Image/BackgroundImages/button.png" height="100" width="100" alt="Alternate Text" /></span>
			</button>
			<button class="carousel-control-prev" type="button" data-bs-target="#carouselExample" data-bs-slide="prev" )">
				<span class="carousel-control-prev-icon"></span>
			</button>
		</div>
		@* practice page *@
		<div class=" carousel-item third">
			<button class="carousel-control-prev" type="button" data-bs-target="#carouselExample" data-bs-slide="prev" )">
				<span class="carousel-control-prev-icon"></span>
			</button>
			<div class="container ">
				<canvas class="practiceBox" id="LetterTrace" height="300" width="300"></canvas>
				<canvas class="practiceBox" id="SelfPractice" height="300" width="300"></canvas>
				<audio id="audio">
					<source src="_content/First_step_For_little_mind.Shared/voice/aa.mp3" type="audio/mpeg">
				</audio>
			</div>
		</div>
	</div>
</div>
<script>
	window.addEventListener("load", function () {
		console.log("DOM is fully loaded");
		setTimeout(() => {
			learningAnimation("myCanvas", "A.png", "Apple.png");
		}, 500)
	});
</script>
